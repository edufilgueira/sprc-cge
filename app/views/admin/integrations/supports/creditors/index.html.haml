- content_for :breadcrumbs, render_breadcrumbs(breadcrumbs)

= form_for admin_integrations_supports_creditors_path, method: :get do |f|

  .card.card-custom

    .card-header

      %h2.card-title
        = t('.title')

    .card-block
      = render 'filters', f: f


    .card-block

      - if creditors.present?
        = render 'table', creditors: creditors

      - else

        .row

          %i.col-md-12.text-center
            = t('.empty')
